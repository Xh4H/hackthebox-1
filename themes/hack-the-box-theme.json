				"foreground": "#FFBD2E"
			}
		},
		{
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#C5D1EB"
			}
		},
		{
			"scope": [
				"support.other.variable"
			],
			"settings": {
				"foreground": "#FF6161"
			}
		},
		{
			"scope": [
				"string.other.link"
			],
			"settings": {
				"foreground": "#FF9A9A"
			}
		},
